// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Car {
  id Int @id @default(autoincrement())
  make String
  model String
  engine String
  year Int
  gearbox String
  drive String
  price String
  mileage Int
  body String
  status Boolean
  main_image String
  slug String @unique
  }